{
  "name": "integration_test",
  "targets": [
    {
      "path": "//regular_integration_tests:text_editing",
      "buildTarget": {
        "name": "text_editing",
        "agentProfiles": [
          "linux", 
          "macos", 
          "ios", 
          "android"
        ],
        dependencies: [
          "//:flutter", //clone the correct version of flutter, enable web.
          "//:similator_installers", // will install/validate emulators.
          "//:web_installers", // will validate drivers and browsers. will start the drivers.
        ],
        browsers: [
          'chrome',
          'safari',
          'ios-safari',
          'android-chrome',
        ],
        drivers: [
          'chrome',
          'safari',
          'ios-safari',
          'android-chrome',
        ],
        "runner": "WebIntegrationTestRunner"
      }
    },
    {
      "path": "//regular_integration_tests:virtual_keyboard",
      "buildTarget": {
        "name": "virtual_keyboard",
        "agentProfiles": [
          "ios", 
          "android"
        ],
        dependencies: [
          "//:flutter", //clone the correct version of flutter, enable web.
          "//:similator_installers", // will install/validate emulators.
          "//:web_installers", // will validate drivers and browsers. will start the drivers.
        ],
        browsers: [
          'ios-safari',
          'android-chrome',
        ],
        drivers: [
          'ios-safari',
          'android-chrome',
        ],
        "runner": "WebIntegrationTestRunner"
      }
    },
  ],
}
